{
  "name": "vauthunters-rewards",
  "version": "1.0.0",
  "description": "Vault Hunters rewards lookup tool",
  "main": "public/index.html",
  "type": "module",
  "scripts": {
    "start": "npm run build:prod && npx --yes wrangler@4.43.0 pages dev dist --compatibility-date=2024-12-10",
    "dev": "npm run build:prod && npx --yes wrangler@4.43.0 pages dev dist --compatibility-date=2024-12-10",
    "build": "node build.js",
    "build:dev": "node build.js --dev",
    "build:watch": "node build.js --dev --watch",
    "build:prod": "node build.js",
    "deploy": "npm run build:prod && npx --yes wrangler@4.43.0 pages deploy dist",
    "lint": "eslint . --ext .js,.mjs",
    "lint:fix": "eslint . --ext .js,.mjs --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "keywords": [
    "vault-hunters",
    "minecraft",
    "rewards"
  ],
  "author": "massuus",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/massuus/Vauthunters-Rewards.git"
  },
  "bugs": {
    "url": "https://github.com/massuus/Vauthunters-Rewards/issues"
  },
  "homepage": "https://vh-rewards.massuus.com",
  "devDependencies": {
    "@eslint/js": "^9.14.0",
    "autoprefixer": "^10.4.22",
    "cssnano": "^7.1.2",
    "esbuild": "^0.24.0",
    "eslint": "^9.14.0",
    "eslint-config-prettier": "^9.1.0",
    "globals": "^15.14.0",
    "postcss": "^8.5.6",
    "postcss-cli": "^11.0.1",
    "prettier": "^3.3.3"
  }
}
